; /etc/stunnel/tinyproxy.conf
; Generated by configure.sh based on .env

pid = /var/run/stunnel.pid
output = /var/log/stunnel.log

[https]
client = no
accept = 443
connect = 127.0.0.1:{{TINYPROXY_PORT}}

cert = {{SSL_CERT_PATH}}
key  = {{SSL_KEY_PATH}}

options = NO_SSLv2
options = NO_SSLv3
options = NO_TLSv1
